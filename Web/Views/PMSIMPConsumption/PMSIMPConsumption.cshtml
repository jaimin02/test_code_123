@{
    ViewBag.Title = "IMP Consumption";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Styles/StyleBlue.css" rel="stylesheet" />
<link href="~/Content/themes/css/CustomStyle.css" rel="stylesheet" />



@using (Html.BeginForm())
{ 
    <input type="hidden" value="@ViewBag.hdnusername" id="hdnusername" />
    <input type="hidden" value="@ViewBag.hdnuserid" id="hdnuserid" />
    <input type="hidden" value="@ViewBag.hdnlogintime" id="hdnlogintime" />
    <input type="hidden" value="@ViewBag.hdnscopevalues" id="hdnscopevalues" />

    <div class="content-wrapper">
        <section class="content-header">
            <h1>IMP Consumption
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dropbox"></i>Products</a></li>
                <li><a href="#">IMP Consumption</a></li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            <div class="row">
                <div class="col-xs-12">
                    <div class="fancy-collapse-panel">
                        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                            <div class="panel panel-default">
                                <div class="panel-heading" role="tab" id="headingOne">
                                    <h4 class="panel-title">
                                        <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                            IMP Consumption
                                        </a>
                                    </h4>
                                </div>
                                <div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
                                    <div class="panel-body">
                                        <div class="box-body" style="overflow-x: hidden">
                                            <div class="col-sm-4 form-group">
                                                <div class="col-sm-12">
                                                    <label>Project No *</label>
                                                </div>
                                                <div class="col-sm-12">
                                                    <input type="text" placeholder="Project No Here.." style="padding: 3px 12px; height: 26px" class="form-control" id="DDLProjectNo" data-provide="typeahead">
                                                </div>
                                            </div>

                                            <div class="col-sm-4 form-group">
                                                <div class="col-sm-12">
                                                    <label>Product Type *</label>
                                                </div>
                                                <div class="col-sm-12">
                                                    <select class="form-control" id="DDLProductType" style="padding: 3px 12px; height: 26px">
                                                        <option value="0">Please Select Product Type</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-sm-4 form-group">
                                                <div class="col-sm-12">
                                                    <label>Product *</label>
                                                </div>
                                                <div class="col-sm-12">
                                                    <select class="form-control" id="DDLProduct" style="padding: 3px 12px; height: 26px">
                                                        <option value="0">Please Select Product</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-sm-4 form-group">
                                                <div class="col-sm-12">
                                                    <label>Batch/Lot/Lot No</label>
                                                </div>
                                                <div class="col-sm-12">
                                                    <select class="form-control" id="ddlBatchLotNo" style="padding: 3px 12px; height: 26px">
                                                        <option value="0">Please Select Batch/Lot/Lot No</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-sm-4 form-group">
                                                <div class="col-sm-12">
                                                    <label>Barcode:</label>
                                                </div>
                                                <div class="col-sm-12">
                                                    @Html.TextBox("txtBarcode", "", new { @class = "form-control", @placeholder = "Barcode No Here..", required = "required", @style = " height:26px; ", @autocomplete = "off" })
                                                </div>
                                            </div>

                                            <div class="col-sm-12 form-group">
                                                <div class="col-sm-12 form-group text-center">

                                                    <a data-toggle="modal" id="btnAdd" data-tooltip="tooltip" title="Add">
                                                        <i class="btn btn-success btn-rounded btn-ef btn-ef-5 btn-ef-5a">
                                                        <i class="glyphicon glyphicon-floppy-saved"></i><span id="spnAdd">Add</span></i>
                                                    </a>

                                                    <a data-toggle="modal" id="btnClear" data-tooltip="tooltip" title="Clear">
                                                        <i class="btn btn-danger btn-rounded btn-ef btn-ef-5 btn-ef-5a">
                                                        <i class="glyphicon glyphicon-refresh"></i><span>Clear</span></i>
                                                    </a>

                                                    <a data-toggle="modal" id="btnExitPage" data-tooltip="tooltip" title="Exit">
                                                        <i class="btn btn-warning btn-rounded btn-ef btn-ef-5 btn-ef-5a">
                                                        <i class="glyphicon glyphicon-floppy-remove"></i><span>Exit</span></i>
                                                    </a>

                                                </div>
                                            </div>

                                            <div class="col-sm-12 form-group">
                                                <div class="col-sm-12 form-group text-center">
                                                    <table id="tblPMSIMPConsumption" class="table table-bordered table-striped dataTable" style="display:none">
                                                        <thead>
                                                            <tr>
                                                                <td>Product Type</td>
                                                                <td>Product</td>
                                                                <td>Batch/Lot/Lot No</td>
                                                                <td>Barcode</td>
                                                                <td class="trDelete">Remove</td>
                                                                <td class="hidetd">nProductType</td>
                                                                <td class="hidetd">nProductNo</td>
                                                                <td class="hidetd">nBatchLotNo</td>
                                                            </tr>
                                                        </thead>
                                                        <tbody id="tbodyIMPConsumption">
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>

                                            <div class="col-sm-12 form-group">
                                                <div class="col-sm-12 form-group text-center">
                                                    <a data-toggle="modal" id="btnSaveIMPConsumption" data-tooltip="tooltip" title="Save" style="display: none">
                                                        <i class="btn btn-success btn-rounded btn-ef btn-ef-5 btn-ef-5a">
                                                        <i class="glyphicon glyphicon-floppy-saved"></i><span id="spnPMSImpConsumption">Save</span></i>
                                                    </a>
                                                </div>
                                            </div>

                                            <div>
                                                <input type="hidden" id="txtbarcode" />
                                                <input type="hidden" id="txtprojectno" />
                                                <input type="hidden" id="txtPeriod" />
                                                <input type="hidden" id="txtSubjectNo" />
                                                <input type="hidden" id="txtDoseNo" />
                                                <input type="hidden" id="txtDayNo" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div id="loader" class="modal"></div>

                        </div>
                    </div>
                    <!-- panel-group -->
                </div>
            </div>
        </section>
    </div>
    
    <div class="modal fade AlertPopup" id="AlertPopup" role="dialog"></div>
    
    <script src="@Url.Content("~/Content/themes/plugins/jQuery/jQuery-2.2.0.min.js")"></script> 
    <script src="~/Scripts/Validation.js"></script>
    <script src="~/Scripts/PMSIMPConsumption.js"></script>
}