@{
    ViewBag.Title = "Product Receipt";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Styles/StyleBlue.css" rel="stylesheet" />


@using (Html.BeginForm())
{
    <div class="content-wrapper">
        <section class="content-header">
            <h1>Product Receipt</h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dropbox"></i>Products</a></li>
                <li><a href="#">Product Receipt</a></li>
            </ol>
        </section>
        <!-- Main content -->
        <section class="content">
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
                        <div class="box-header">
                            <div class="row">
                                <div class="col-sm-12 form-group">
                                    <span>
                                        <canvas id="myCanvas" width="12" height="12" class="highlightSaveContinue"></canvas>
                                    </span>
                                    <span class="InActive">Save & Continue Entries</span>
                                    &nbsp;&nbsp;&nbsp;
                                    @* <span>
                                               <canvas id="myCanvas" width="12" height="12" class="highlightPendingQA"></canvas>
                                           </span>
                                        <span class="InActive">QA Review Approved </span>
                                        &nbsp;&nbsp;&nbsp;
                                         <span>
                                             <canvas id="myCanvas1" width="12" height="12" class="highlightRejectedQA"></canvas>
                                         </span>
                                        <span class="InActive">QA Review Rejected</span>
                                        &nbsp;&nbsp;&nbsp;*@
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                        <h3 class="box-title">Project No</h3>
                                    </div>
                                    <div class="col-lg-4 col-md-10 col-sm-10 col-xs-10">
                                        <input type="text" id="ddlProjectNodashboard" placeholder="Project No.." data-provide="typeahead" class="form-control" autocomplete="off" />
                                    </div>
                                    @*<div class="col-lg-4 col-lg-offset-0 col-md-6 col-md-offset-2 col-sm-6 col-sm-offset-2 col-xs-10 resizepd">
                                            <div id="divexport" style="visibility: hidden">
                                                <a class="btn bg-olive" href="../PmsStudyProductReceipt/ExportToExcel/hdnworkspaceid">Export to Excel&nbsp&nbsp&nbsp&nbsp<i class="fa fa-file-excel-o"></i></a>
                                            </div>
                                        </div>*@

                                    <div class="col-lg-4 col-lg-offset-0 col-md-6 col-md-offset-2 col-sm-6 col-sm-offset-2 col-xs-10 resizepd btn-group">
                                        <button type="button" class="btn bg-olive dropbtn dropdown-toggle btn-rounded  btn-ef btn btn-primary" id="divexport"
                                                data-toggle="dropdown" aria-expanded="false" style="border-radius: 4px">
                                            Export <span class="caret"></span>
                                        </button>
                                        <ul class="dropdown-menu" role="menu">
                                            <li><a href="../PmsStudyProductReceipt/ExportToExcel"><i class="fa fa-file-excel-o"></i>Excel </a></li>
                                            <li><a href="../PmsStudyProductReceipt/ExportToPDF"><i class="fa fa-file-pdf-o"></i>PDF</a></li>
                                        </ul>
                                    </div>

                                    @if (!System.Configuration.ConfigurationManager.AppSettings["UserProfile"].Contains(ViewBag.hdnUserTypeCode))
                                    {
                                        <div class="col-lg-2 col-lg-offset-0 col-md-3 col-md-offset-1 col-sm-3 col-sm-offset-1 col-xs-10 resizepd">
                                            <button type="button" id="btnAddStudyProduct" class="btn bg-olive viewmode" data-toggle="modal" data-target="#ProductReceiptModel"
                                                    title="Add Product Receipt">
                                                + Add Product Receipt
                                            </button>
                                        </div>
                                    }

                                </div>
                            </div>
                        </div>

                        <!-- /.box-header -->
                        <div class="box-body">
                            <table id="tblPmsProductReceiptData" class="table table-bordered table-striped dt-head-right"></table>
                        </div>
                        <!-- /.box-body -->
                    </div>
                </div>
            </div>

            <div class="modal fade" id="ProductReceiptModel" role="dialog">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <div>
                                <button type="button" class="Close" onclick="cleardocumentspart()" data-dismiss="modal">&times;</button>
                                <span class="operation-mode" id="titleMode"></span>
                                <h4 class="modal-title modaltitle operation-name" id="title">Product Receipt</h4>
                            </div>
                            <div>
                                <h5><span class="fields-mandatory">Fields with * are Mandatory</span></h5>
                            </div>
                        </div>
                        <div class="modal-body" style="height: 430px; overflow-y: auto">

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Project No *</label>
                                </div>
                                <div class="col-sm-12">
                                    <input type="text" id="ProjectNo" placeholder="Project No.." data-provide="typeahead" class="form-control" style="height: 26px" />
                                </div>
                            </div>
                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Storage Name *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control" id="StorageName" style="padding: 3px 12px; height: 26px"></select>
                                </div>
                            </div>
                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Received From Location</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("RecdFromLocation", "", new { @class = "form-control headercontrol", @placeholder = "Recd. From Location", @style = " height:26px; ", @autocomplete = "off" })
                                </div>
                            </div>
                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Receipt Date & Time *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("txtReceiptDateTime", "", new { @class = "form-control headercontrol", @placeholder = "Receipt Date & Time", @style = " height:26px; ", @autocomplete = "off", @onpaste = "return false" })
                                </div> 
                            </div>
                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>HAWB No</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("RefNo", "", new { @class = "form-control headercontrol", @placeholder = "HAWB No", @style = " height:26px; ", @autocomplete = "off" })
                                </div>
                            </div>
                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Shipment No</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("ShipmentNo", "", new { @class = "form-control headercontrol", @placeholder = "Shipment No", @style = " height:26px; ", @autocomplete = "off" })
                                </div>
                            </div>
                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>IMP/Non-IMP Product type</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control headercontrol" id="ddlImpProductType" style="padding: 3px 12px; height: 26px">
                                        <option selected="selected" value="0">Please Select IMP/Non-IMP Product type</option>
                                        <option value="Y">IMP</option>
                                        <option value="N">Non-IMP</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Product Type *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control headercontrol" id="ddlProductType" style="padding: 3px 12px; height: 26px">
                                        <option selected="selected" value="0">Please Select Product Type</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Product Indication *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control headercontrol" id="ddlTransferIndi" style="padding: 3px 12px; height: 26px">
                                        <option selected="selected" value="0">Please Select Product Indication</option>
                                        <option value="P">Product</option>
                                        <option value="K">Kit</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Condition of Pack Received</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("txtConditionofPackRecd", "", new { @class = "form-control headercontrol", @placeholder = "Condition of Pack Received", @style = " height:26px; ", @autocomplete = "off" })
                                </div>
                            </div>
                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Transporter Name *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control headercontrol" id="TransportMode" style="padding: 3px 12px; height: 26px">
                                        <option value="0" selected="selected">Please Select Transporter Name</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divOtherTransportMode" style="display: none">
                                <div class="col-sm-12">
                                    <label>Other Transport Name *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("vOtherTransportMode", "", new { @class = "form-control headercontrol", @placeholder = "Transport Name", @style = " height:26px; " })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divTransferReceiptType" style="display: none">
                                <div class="col-sm-12">
                                    <label>Mode of Receipt *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control headercontrol" id="ddlReceiptType" style="padding: 3px 12px; height: 26px">
                                        <option value="0" selected="selected">Please Select Mode of Receipt</option>
                                        <option value="D">Direct Receipt</option>
                                        <option value="T">By Transfer</option>
                                    </select>
                                </div>
                            </div>


                            <div class="box pull-left">
                                <label>Verification Of IMP Quantity Received</label>
                            </div>



                            <div class="col-sm-4 form-group hideproduct">
                                <div class="col-sm-12">
                                    <label>Product *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control" id="Product" style="padding: 3px 12px; height: 26px">
                                        <option value="0">Please Select Product</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group hideproduct">
                                <div class="col-sm-12">
                                    <label>Batch/Lot/Lot No *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="BatchLotNo" style="padding: 3px 12px; height: 26px">
                                        <option value="0">Please Select Batch/Lot/Lot No</option>
                                    </select>
                                </div>
                            </div>


                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Re-Test/Expiry/Provisional Expiry Date</label>
                                </div>
                                <div class="col-sm-12">
                                    <input type="text" class="form-control pull-right" id="ExpiryDate" placeholder="Re-Test/Expiry/Provisional Expiry Date" style="height: 26px" disabled="disabled" onpaste="return false" autocomplete="off">
                                </div>
                            </div>

                            <div class="col-sm-4 form-group hideproduct">
                                <div class="col-sm-12">
                                    <label>No. of Boxes *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("NoofBoxes", "", new { @class = "form-control qtycontrol", @placeholder = "No of Boxes", @style = " height:26px; ", @onblur = "RecdQtyfunction()", @type = "number", @min = "0", @autocomplete = "off" })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" style="display:none">
                                <div class="col-sm-12">
                                    <label>No. of Qty/Boxes *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("NoofQty", "", new { @class = "form-control qtycontrol", @placeholder = "No. of Qty/Boxes", @style = " height:26px; ", @onblur = "RecdQtyfunction()", @type = "number", @min = "0", @autocomplete = "off" })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divRecdQty">
                                <div class="col-sm-12">
                                    <label id="lblRecdQty"></label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("RecdQty", "", new { @class = "form-control qtycontrol", @placeholder = "Recd. Quantity", @style = " height:26px;", @type = "number", @min = "0" })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divUnit">
                                <div class="col-sm-12">
                                    <label>Unit of materials *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="ddlUnit" style="padding: 3px 12px; height: 26px">
                                        <option value="0">Please Select Unit of materials</option>
                                        <option value="%">%</option>
                                        @*<option value="Deg. F">Deg. F</option>*@
                                        <option value="Nos.">Nos.</option>
                                        @*<option value="ml">ml</option>*@
                                        <option value="kg">kg</option>
                                        <option value="gm">gm</option>
                                        <option value="Tablet">Tablet</option>
                                        <option value="Capsule">Capsule</option>
                                        <option value="Bottle">Bottle</option>
                                        @*<option value="Deg. Cel">Deg. Cel</option>
                                            <option value="%RH">%RH</option>
                                            <option value="RPM">RPM</option>
                                            <option value="RCF">RCF</option>*@
                                        <option value="PFS">PFS</option>
                                        <option value="Syringe">Syringe</option>
                                        <option value="vials">vials</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group hidekit">
                                <div class="col-sm-12">
                                    <label>No of Kit *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("txtNoofKit", "", new { @class = "form-control qtycontrol", @placeholder = "No of Kit", @style = " height:26px;", @type = "number", @min = "0", @autocomplete = "off" })
                                </div>
                            </div>



                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Condition of Products Packs</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="ddlConditionofProducts" style="padding: 3px 12px; height: 26px">
                                        <option value="0">Please Select Condition of Products</option>
                                        <option value="S">Sealed</option>
                                        <option value="U">Unsealed</option>
                                        <option value="D">Damage</option>
                                    </select>
                                    @*@Html.TextBox("txtConditionofProducts", "", new { @class = "form-control", @placeholder = "Condition of Products", @style = " height:26px;" })*@
                                </div>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Purpose</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="ddlPurpose" style="padding: 3px 12px; height: 26px">
                                        <option value="Please Select Purpose">Please Select Purpose</option>
                                        <option value="Clinical Study">Clinical Study</option>
                                        <option value="In Vitro">In Vitro</option>
                                        <option value="Storage">Storage</option>
                                        <option value="Other">Other</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divOtherPurpose" style="display: none">
                                <div class="col-sm-12">
                                    <label>Other Purpose Name *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("vOtherPurpose", "", new { @class = "form-control headercontrol", @placeholder = "Purpose Name", @style = " height:26px; ", @autocomplete = "off" })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Temperature Data logger received</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="ddlTempRecd" style="padding: 3px 12px; height: 26px">
                                        <option value="Y">Yes</option>
                                        <option value="N">No</option>
                                        <option value="NA">NA</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divTempCount">
                                <div class="col-sm-12">
                                    <label>Temperature data logger count *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("tempCount", "", new { @class = "form-control", @placeholder = "Temperature data logger count", @style = " height:26px; ", @type = "number", @min = "0", @autocomplete = "off" })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divTempNumber">
                                <div class="col-sm-12">
                                    <label>Temperature data logger number *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("tempNumber", "", new { @class = "form-control", @placeholder = "Temperature data logger number", @style = " height:26px; ", @min = "0", @autocomplete = "off" })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>COA received</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="ddlCoaRecd" style="padding: 3px 12px; height: 26px">
                                        <option value="Y">Yes</option>
                                        <option value="N">No</option>
                                        <option value="NA">NA</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-4 form-group" style="display:none">
                                <div class="col-sm-12">
                                    <label>GMP Certificate received</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="ddlGmpRecd" style="padding: 3px 12px; height: 26px">
                                        <option value="Y">Yes</option>
                                        <option value="N">No</option>
                                        <option value="NA">NA</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divCOAdate">
                                <div class="col-sm-12">
                                    <label>COA/COC Receipt date *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("COADate", "", new { @class = "form-control", @placeholder = "COA/COC Receipt Date", @style = " height:26px; ", @autocomplete = "off", @onpaste = "return false", @onkeydown = "return false" })
                                    @*<input type="text" class="form-control pull-right" id="COADate" placeholder="COA Date" style="height: 26px">*@
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divGMPdate">
                                <div class="col-sm-12">
                                    <label>GMP Receipt date *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("GMPDate", "", new { @class = "form-control", @placeholder = "GMP Receipt Date", @style = " height:26px;display:none; ", @autocomplete = "off", @onpaste = "return false", @onkeydown = "return false" })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Type of Storage*</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="ddlSelectStorage" style="padding: 3px 12px; height: 26px">
                                        <option value="0">Select type of storage</option>
                                        <option value="Bin">Bin</option>
                                        <option value="Pallet">Pallet</option>
                                        <option value="Crate">Crate</option>
                                        <option value="Locker">Locker</option>
                                        <option value="Container">Container</option>
                                        <option value="Shelf">Shelf</option>
                                        <option value="Box">Box</option>
                                        <option value="Boxes">Boxes</option>
                                        <option value="Rack">Rack</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divStorage" style="display:none">
                                <div class="col-sm-12">
                                    <label id="lblstorage">No Bin/Pallet/Crate/Locker/Container/Shelf/Box/Boxes/Rack *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextBox("txtNoOfContainers", "", new { @class = "form-control qtycontrol", @placeholder = "No Bin/Pallet/Crate/Locker/Container/Shelf/Box/Boxes/Rack", @style = " height:26px;", @type = "number", @min = "0", @autocomplete = "off" })
                                </div>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Storage Area *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select id="StorageArea" multiple="multiple"></select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label id="lblRemarks">Remarks *</label>
                                </div>
                                <div class="col-sm-12">
                                    @Html.TextArea("txtRemarks", "", new { @class = "form-control", @placeholder = "Remarks", @autocomplete = "off" })
                                </div>
                            </div>

                            <div class="box pull-left">
                                <label>Document Upload</label>
                            </div>

                            <div class="col-sm-4 form-group">
                                <div class="col-sm-12">
                                    <label>Select Type of Document *</label>
                                </div>
                                <div class="col-sm-12">
                                    <select class="form-control select2" id="ddlSelectDocument" style="padding: 3px 12px; height: 26px">
                                        <option value="0">Select type of Document</option>
                                        <option value="AWB copy">AWB copy</option>
                                        <option value="Invoice">Invoice</option>
                                        <option value="Material gate pass">Material gate pass</option>
                                        <option value="Delivery challan">Delivery challan</option>
                                        <option value="Inbound packaging list">Inbound packaging list</option>
                                        <option value="Covering letter">Covering letter</option>
                                        <option value="Acknowledgement letter from consigner">Acknowledgement letter from consigner</option>
                                        <option value="Non-hazardous certificate">Non-hazardous certificate</option>
                                        <option value="MSDS">MSDS</option>
                                        <option value="COA">COA</option>
                                        <option value="COC">COC</option>
                                        <option value="Stability data">Stability data</option>
                                        <option value="CT approval">CT approval</option>
                                        <option value="Import license">Import license</option>
                                        <option value="Other">Other</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4 form-group" id="divUploadDocument" style="display:none">
                                <div class="col-sm-12">
                                    <label>Upload Document *</label>
                                </div>
                                <div class="col-sm-12">
                                    <div class="col-sm-10">
                                        @Html.TextBox("uploadDocument", "", new { @class = "form-control", @placeholder = "Upload Document *", @style = "height:26px; padding: 1px", @autocomplete = "off", @type = "file" })
                                        
                                    </div>
                                    <div class="col-sm-2">
                                        <a id="btnUpload" title="Upload" style="margin: 0!important" class="btn btn-primary btn-rounded btn-ef btn-ef-5 btn-ef-5a bg-olive margin">
                                            <i class="fa fa-plus"></i><span>Upload</span>
                                        </a>
                                    </div>
                                </div>
                            </div>

